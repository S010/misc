source /etc/vimrc

colorscheme murphy
set gfn=Terminus\ 12
"set guioptions+=c " console dialogs for simple choices
"set guioptions+=v " vertical button layout for dialogs
"set guioptions+=b " bottom scrollbar
"set guioptions+=h " limit the length of bottom scrollbar to length of cursor
"line: less computations
set guioptions-=m " no menubar
set guioptions-=T " no toolbar
" no scrollbars
set guioptions-=r
set guioptions-=R
set guioptions-=l 
set guioptions-=L
hi CursorLine guibg=#121f12
set cursorline

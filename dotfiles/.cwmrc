command term "xterm -geometry 110x40"
moveamount 8 
snapdist 8
bind M-Prior "volume.sh +"
bind M-Next "volume.sh -"
bind XF86AudioRaiseVolume "volume.sh +"
bind XF86AudioLowerVolume "volume.sh -"
bind XF86AudioMute "volume.sh mute"
#bind XF86MonBrightnessUp "brightness +"
#bind XF86MonBrightnessDown "brightness -"
borderwidth 2
bind M-period unmap
bind M-comma ssh
bind CM-o "i3lock"
color menubg rgb:00/2b/36
color menufg rgb:83/94/96
color font rgb:83/94/96
color inactiveborder rgb:58/6e/75
color activeborder rgb:93/a1/a1
